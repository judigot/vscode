            "    [[ \"\\$value\" =~ ^[0-9]+\\$ ]] && json+=\"\\$value,\" || json+=\"\\\"\\$value\\\",\"",
            "done",
            "json=\"\\${json%,}}\"",
            "echo \"\\$json\""
        ],
        "description": "Convert Array/Object to String (Shell)",
        "scope": "shellscript"
